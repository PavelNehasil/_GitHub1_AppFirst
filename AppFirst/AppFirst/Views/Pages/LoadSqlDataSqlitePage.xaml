<?xml version="1.0" encoding="utf-8" ?>
<Page
    x:Class="AppFirst.Views.Pages.LoadSqlDataSqlitePage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:i="using:Microsoft.Xaml.Interactivity"
    xmlns:ic="using:Microsoft.Xaml.Interactions.Core"
    xmlns:local="using:AppFirst"
    xmlns:local1="using:AppFirst.Models"
    xmlns:local2="using:AppFirst.Views.Pages"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:usercontrols="using:AppFirst.Controls"
    xmlns:vm="using:AppFirst.ViewModels.Pages"
    xmlns:wuc="using:WinUICommunity"
    mc:Ignorable="d">

    <Page.Resources>
        <StandardUICommand
            x:Name="EditUserCommand"
            Command="{x:Bind ViewModel.EditUserCommand}"
            Description="Edit user" />
        <StandardUICommand
            x:Name="DeleteUserCommand"
            Command="{x:Bind ViewModel.DeleteUserCommand}"
            Description="Delete user" />
        <StandardUICommand
            x:Name="DeleteLoginUserCommand"
            Command="{x:Bind ViewModel.DeleteLoginUserCommand}"
            Description="Delete login user" />
        <StandardUICommand
            x:Name="DeleteLoginImageCommand"
            Command="{x:Bind ViewModel.DeleteLoginImageCommand}"
            Description="Delete login image" />

        <DataTemplate x:Key="UsersListViewTemplate" x:DataType="local1:User">
            <UserControl PointerEntered="ListViewSwipeContainer_PointerEntered" PointerExited="ListViewSwipeContainer_PointerExited">
                <RelativePanel
                    Width="1200"
                    Height="60"
                    Padding="4,4,4,4"
                    Background="Transparent">
                    <StackPanel Background="Transparent" Orientation="Horizontal">
                        <StackPanel
                            Margin="1"
                            Padding="1"
                            Background="Transparent"
                            Orientation="Horizontal">
                            <StackPanel
                                Width="100"
                                VerticalAlignment="Stretch"
                                HorizontalAlignment="Center">
                            <PersonPicture
                                Width="50"
                                VerticalAlignment="Center"
                                HorizontalAlignment="Left"
                                ProfilePicture="{Binding ImageSource}" />
                            </StackPanel>
                            <TextBlock
                                VerticalAlignment="Center"
                                HorizontalAlignment="Right"
                                Width="100"
                                Margin="5,0,0,0"
                                Style="{ThemeResource BaseTextBlockStyle}"
                                Text="{Binding Id}" />
                            <TextBlock
                                VerticalAlignment="Center"
                                Width="150"
                                Margin="5,0,0,0"
                                Style="{ThemeResource BaseTextBlockStyle}"
                                Text="{Binding UserName}" />
                            <TextBlock
                                VerticalAlignment="Center"
                                Width="150"
                                Margin="5,0,0,0"
                                Style="{ThemeResource BaseTextBlockStyle}"
                                Text="{Binding Password}" />
                            <CheckBox
                                VerticalAlignment="Center"
                                Width="50"
                                Margin="5,0,0,0"
                                IsChecked="{Binding IsAdmin, Mode=OneWay}"
                                IsEnabled="False"
                                Style="{ThemeResource BaseTextBlockStyle}" />
                            <TextBlock
                                VerticalAlignment="Center"
                                Width="200"
                                Margin="5,0,0,0"
                                Style="{ThemeResource BaseTextBlockStyle}"
                                Text="{Binding Email}" />
                            <TextBlock
                                VerticalAlignment="Center"
                                Width="100"
                                Margin="5,0,0,0"
                                Style="{ThemeResource BaseTextBlockStyle}"
                                Text="{Binding FirstName}" />
                            <TextBlock
                                VerticalAlignment="Center"
                                Width="100"
                                Margin="5,0,0,0"
                                Style="{ThemeResource BaseTextBlockStyle}"
                                Text="{Binding LastName}" />
                        </StackPanel>
                        <StackPanel
                            x:Name="HoverButtons"
                            Background="Transparent"
                            Orientation="Horizontal"
                            RelativePanel.AlignRightWithPanel="True"
                            RelativePanel.AlignVerticalCenterWithPanel="True"
                            Visibility="Collapsed">
                            <AppBarButton
                                Width="32"
                                Command="{StaticResource EditUserCommand}"
                                CommandParameter="{x:Bind Id}"
                                Icon="Edit"
                                IsCompact="True"
                                ToolTipService.ToolTip="Edit user" />
                            <AppBarButton
                                Width="32"
                                Command="{StaticResource DeleteUserCommand}"
                                CommandParameter="{x:Bind Id}"
                                Icon="Delete"
                                IsCompact="True"
                                ToolTipService.ToolTip="Delete user" />
                        </StackPanel>
                    </StackPanel>
                    <VisualStateManager.VisualStateGroups>
                        <VisualStateGroup x:Name="HoveringStates">
                            <VisualState x:Name="HoverButtonsHidden" />
                            <VisualState x:Name="HoverButtonsShown">
                                <VisualState.Setters>
                                    <Setter Target="HoverButtons.Visibility" Value="Visible" />
                                </VisualState.Setters>
                            </VisualState>
                        </VisualStateGroup>
                    </VisualStateManager.VisualStateGroups>
                </RelativePanel>
            </UserControl>
        </DataTemplate>
        <DataTemplate x:Key="LoginUsersListViewTemplate" x:DataType="local1:LoginUser">
            <UserControl PointerEntered="ListViewSwipeContainer_PointerEntered" PointerExited="ListViewSwipeContainer_PointerExited">
                <RelativePanel
                    Height="55"
                    Padding="8,4,4,4"
                    Background="Transparent">
                    <StackPanel Background="Transparent" Orientation="Horizontal">
                        <StackPanel
                            Margin="5"
                            Padding="7"
                            Background="Transparent"
                            Orientation="Horizontal">
                            <TextBlock
                                Width="100"
                                Margin="5,0,0,0"
                                Style="{ThemeResource BaseTextBlockStyle}"
                                Text="{Binding IdUser}" />
                            <TextBlock
                                Width="100"
                                Margin="5,0,0,0"
                                Style="{ThemeResource BaseTextBlockStyle}"
                                Text="{Binding IdLoginType}" />
                            <TextBlock
                                Width="100"
                                Margin="5,0,0,0"
                                Style="{ThemeResource BaseTextBlockStyle}"
                                Text="{Binding LoginType}" />
                            <TextBlock
                                Width="150"
                                Margin="5,0,0,0"
                                Style="{ThemeResource BaseTextBlockStyle}"
                                Text="{Binding Date}" />
                        </StackPanel>
                        <StackPanel
                            x:Name="HoverButtonsDetail"
                            Background="Transparent"
                            Orientation="Horizontal"
                            RelativePanel.AlignRightWithPanel="True"
                            RelativePanel.AlignVerticalCenterWithPanel="True"
                            Visibility="Collapsed">

                            <AppBarButton
                                Width="40"
                                Height="40"
                                Command="{StaticResource DeleteLoginUserCommand}"
                                CommandParameter="{x:Bind Date}"
                                Icon="Delete"
                                IsCompact="True"
                                ToolTipService.ToolTip="Delete login user" />
                        </StackPanel>
                    </StackPanel>
                    <VisualStateManager.VisualStateGroups>
                        <VisualStateGroup x:Name="HoveringStatesDetail">
                            <VisualState x:Name="HoverButtonsHidden" />
                            <VisualState x:Name="HoverButtonsShown">
                                <VisualState.Setters>
                                    <Setter Target="HoverButtonsDetail.Visibility" Value="Visible" />
                                </VisualState.Setters>
                            </VisualState>
                        </VisualStateGroup>
                    </VisualStateManager.VisualStateGroups>
                </RelativePanel>
            </UserControl>
        </DataTemplate>
        <DataTemplate x:Key="LoginImagesListViewTemplate" x:DataType="local1:LoginImage">
            <Grid Margin="0,1,0,1" AutomationProperties.Name="{x:Bind ImageName}">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" MinWidth="150" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <PersonPicture
                    x:Name="ImagePersonPicture"
                    MaxHeight="100"
                    ProfilePicture="{x:Bind ImageSource}" />
                <StackPanel Grid.Column="1" Margin="12,0,0,0">
                    <TextBlock
                        Margin="0,0,0,6"
                        HorizontalAlignment="Left"
                        FontSize="14"
                        FontWeight="SemiBold"
                        LineHeight="20"
                        Style="{ThemeResource BaseTextBlockStyle}"
                        Text="{x:Bind ImageName}" />
                    <TextBlock
                        Width="350"
                        MaxHeight="60"
                        Margin="0,0,0,10"
                        FontFamily="Segoe UI"
                        FontSize="13"
                        FontWeight="Normal"
                        Style="{ThemeResource BodyTextBlockStyle}"
                        Text="{x:Bind Description}"
                        TextTrimming="CharacterEllipsis"
                        TextWrapping="Wrap" />
                    <Button
                        x:Name="TestButton3"
                        Click="TestButton3Click"
                        Content="TeachingTip" />
                    <usercontrols:AutoCloseTeachingTip
                        x:Name="ToggleThemeTeachingTip3"
                        Title="{x:Bind ImageName}"
                        PreferredPlacement="Center"
                        Subtitle="..."
                        Target="{Binding ElementName=TestButton3}">
                        <usercontrols:AutoCloseTeachingTip.HeroContent>
                            <Image AutomationProperties.Name="Sunset" Source="{x:Bind ImageSource}" />
                        </usercontrols:AutoCloseTeachingTip.HeroContent>
                    </usercontrols:AutoCloseTeachingTip>
                </StackPanel>
            </Grid>

        </DataTemplate>
    </Page.Resources>

    <Grid>
        <Grid ColumnDefinitions="*" RowDefinitions="Auto,450,*">
            <StackPanel
                Grid.Row="0"
                Margin="0,20,0,5"
                HorizontalAlignment="Stretch"
                VerticalAlignment="Center"
                Orientation="Horizontal">
                <Button
                    Margin="10,5,5,5"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Command="{Binding ShowSqlConnectionStringDialogCommand, Mode=OneWay}"
                    Content="ConnectionString" />
                <TextBox
                    Height="NaN"
                    Margin="5,5,0,5"
                    VerticalAlignment="Center"
                    AcceptsReturn="False"
                    IsEnabled="True"
                    Text="{Binding ConnectionString}" />
            </StackPanel>

            <StackPanel
                Grid.Row="1"
                VerticalAlignment="Top"
                Orientation="Vertical">
                <StackPanel VerticalAlignment="Top" Orientation="Horizontal">
                    <CommandBar
                        Margin="10,0,0,0"
                        Background="Transparent"
                        DefaultLabelPosition="Right"
                        FontSize="14"
                        IsOpen="False">
                        <AppBarButton
                            x:Name="AddButton"
                            x:Uid="AddButton"
                            Command="{x:Bind ViewModel.AddUserDialogCommand}"
                            Icon="Add"
                            Label="Add"
                            ToolTipService.ToolTip="Add user" />
                        <AppBarSeparator />
                        <AppBarButton
                            x:Uid="RefreshButton"
                            Command="{x:Bind ViewModel.ReloadCommand}"
                            Icon="Refresh"
                            ToolTipService.ToolTip="Refresh users" />
                        <AppBarSeparator />
                        <AppBarButton
                            Command="{x:Bind ViewModel.EditUserDialogCommand}"
                            Content="Edit"
                            Icon="Edit"
                            IsEnabled="{x:Bind ViewModel.IsUserSelected, Mode=OneWay}"
                            ToolTipService.ToolTip="Edit user" />
                        <AppBarButton
                            Command="{x:Bind ViewModel.DeleteUserDialogCommand}"
                            Content="Delete"
                            Icon="Delete"
                            IsEnabled="{x:Bind ViewModel.IsUserSelected, Mode=OneWay}"
                            ToolTipService.ToolTip="Delete user" />
                        <AppBarSeparator />
                        <AppBarButton
                            Command="{x:Bind ViewModel.MoveUpUserCommand}"
                            Content="MoveUp"
                            IsEnabled="{x:Bind ViewModel.IsUserSelected, Mode=OneWay}"
                            ToolTipService.ToolTip="Move up selected user">
                            <AppBarButton.Icon>
                                <FontIcon Glyph="&#xE70E;" />
                            </AppBarButton.Icon>
                        </AppBarButton>
                        <AppBarButton
                            Command="{x:Bind ViewModel.MoveDownUserCommand}"
                            Content="MoveDown"
                            IsEnabled="{x:Bind ViewModel.IsUserSelected, Mode=OneWay}"
                            ToolTipService.ToolTip="Move down selected user">
                            <AppBarButton.Icon>
                                <FontIcon Glyph="&#xE70D;" />
                            </AppBarButton.Icon>
                        </AppBarButton>
                        <AppBarSeparator />
                        <CommandBar.SecondaryCommands>
                            <AppBarButton Icon="Setting" Label="Settings">
                                <AppBarButton.KeyboardAccelerators>
                                    <KeyboardAccelerator Key="I" Modifiers="Control" />
                                </AppBarButton.KeyboardAccelerators>
                            </AppBarButton>
                        </CommandBar.SecondaryCommands>
                    </CommandBar>
                    <AutoSuggestBox
                        MinWidth="320"
                        VerticalAlignment="Center"
                        PlaceholderText="Search"
                        QueryIcon="Find"
                        TextChanged="OnSearchCommand" />
                </StackPanel>

                <StackPanel
                    HorizontalAlignment="Stretch"
                    VerticalAlignment="Top"
                    Orientation="Horizontal">

                    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">

                        <ListView
                            x:Name="UsersListView"
                            HorizontalAlignment="Stretch"
                            Width="1200"
                            Height="400"
                            Margin="10,0,0,0"
                            BorderBrush="{ThemeResource SystemControlForegroundBaseMediumLowBrush}"
                            BorderThickness="1"
                            CanReorderItems="True"
                            ItemTemplate="{StaticResource UsersListViewTemplate}"
                            ItemsSource="{x:Bind ViewModel.TableUsers, Mode=TwoWay}"
                            SelectedItem="{x:Bind ViewModel.SelectedUser, Mode=TwoWay}"
                            SelectionMode="Single">
                            <ListView.Header>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock
                                        Width="50"
                                        Margin="5,0,0,0"
                                        VerticalAlignment="Center"
                                        FontWeight="Bold"
                                        HorizontalTextAlignment="Center"
                                        Text="Image" />
                                    <TextBlock
                                        Width="100"
                                        Margin="5,0,0,0"
                                        VerticalAlignment="Center"
                                        FontWeight="Bold"
                                        HorizontalTextAlignment="Center"
                                        Text="ID" />
                                    <TextBlock
                                        Width="150"
                                        Margin="5,0,0,0"
                                        VerticalAlignment="Center"
                                        FontWeight="Bold"
                                        HorizontalTextAlignment="Center"
                                        Text="UserName" />
                                    <TextBlock
                                        Width="150"
                                        Margin="5,0,0,0"
                                        VerticalAlignment="Center"
                                        FontWeight="Bold"
                                        HorizontalTextAlignment="Center"
                                        Text="Password" />
                                    <TextBlock
                                        Width="50"
                                        Margin="5,0,0,0"
                                        VerticalAlignment="Center"
                                        FontWeight="Bold"
                                        HorizontalTextAlignment="Center"
                                        Text="IsAdmin" />
                                    <TextBlock
                                        Width="200"
                                        Margin="5,0,0,0"
                                        VerticalAlignment="Center"
                                        FontWeight="Bold"
                                        HorizontalTextAlignment="Center"
                                        Text="Email" />
                                    <TextBlock
                                        Width="100"
                                        Margin="5,0,0,0"
                                        VerticalAlignment="Center"
                                        FontWeight="Bold"
                                        HorizontalTextAlignment="Center"
                                        Text="FirstName" />
                                    <TextBlock
                                        Width="100"
                                        Margin="5,0,0,0"
                                        VerticalAlignment="Center"
                                        FontWeight="Bold"
                                        HorizontalTextAlignment="Center"
                                        Text="LastName" />
                                </StackPanel>
                            </ListView.Header>
                        </ListView>
                    </ScrollViewer>
                </StackPanel>
            </StackPanel>

            <StackPanel
                Grid.Row="2"
                VerticalAlignment="Top"
                Orientation="Vertical">
                <StackPanel VerticalAlignment="Top" Orientation="Horizontal">
                    <CommandBar
                        Margin="10,0,0,0"
                        Background="Transparent"
                        DefaultLabelPosition="Right"
                        FontSize="14"
                        IsOpen="False">
                        <AppBarButton
                            x:Uid="RefreshButton"
                            Command="{x:Bind ViewModel.ReloadLoginUsersCommand}"
                            Icon="Refresh"
                            ToolTipService.ToolTip="Refresh users" />
                        <AppBarSeparator />
                        <AppBarButton
                            Command="{x:Bind ViewModel.DeleteLoginUserIdCommand}"
                            Content="Delete"
                            Icon="Delete"
                            IsEnabled="{x:Bind ViewModel.IsUserSelected, Mode=OneWay}"
                            ToolTipService.ToolTip="Delete user" />
                        <AppBarSeparator />
                        <CommandBar.SecondaryCommands>
                            <AppBarButton Icon="Setting" Label="Settings">
                                <AppBarButton.KeyboardAccelerators>
                                    <KeyboardAccelerator Key="I" Modifiers="Control" />
                                </AppBarButton.KeyboardAccelerators>
                            </AppBarButton>
                        </CommandBar.SecondaryCommands>
                    </CommandBar>
                </StackPanel>

                <StackPanel
                    HorizontalAlignment="Left"
                    VerticalAlignment="Top"
                    Orientation="Horizontal">

                    <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">

                        <ListView
                            x:Name="LoginUsersListView"
                            Width="600"
                            Height="400"
                            Margin="10,0,0,0"
                            BorderBrush="{ThemeResource SystemControlForegroundBaseMediumLowBrush}"
                            BorderThickness="1"
                            CanReorderItems="True"
                            ItemTemplate="{StaticResource LoginUsersListViewTemplate}"
                            ItemsSource="{x:Bind ViewModel.TableLoginUsers, Mode=TwoWay}"
                            SelectedItem="{x:Bind ViewModel.SelectedLoginUser, Mode=TwoWay}"
                            SelectionMode="Single">
                            <ListView.ContextFlyout>
                                <MenuFlyout>
                                    <MenuFlyoutItem
                                        x:Uid="CreateLoginEvent"
                                        Command="{x:Bind ViewModel.CreateLoginEventCommand}"
                                        CommandParameter="{x:Bind}"
                                        Text="Login event">
                                        <MenuFlyoutItem.Icon>
                                            <FontIcon Glyph="&#xE77B;" />
                                        </MenuFlyoutItem.Icon>
                                    </MenuFlyoutItem>
                                    <MenuFlyoutItem
                                        x:Uid="CreatePasswordEvent"
                                        Command="{x:Bind ViewModel.CreatePasswordEventCommand}"
                                        CommandParameter="{x:Bind}"
                                        Text="Password event">
                                        <MenuFlyoutItem.Icon>
                                            <FontIcon Glyph="&#xE8F8;" />
                                        </MenuFlyoutItem.Icon>
                                    </MenuFlyoutItem>
                                </MenuFlyout>
                            </ListView.ContextFlyout>
                            <ListView.Header>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock
                                        Width="100"
                                        Margin="5,0,0,0"
                                        VerticalAlignment="Center"
                                        FontWeight="Bold"
                                        HorizontalTextAlignment="Center"
                                        Text="IdUser" />
                                    <TextBlock
                                        Width="100"
                                        Margin="5,0,0,0"
                                        VerticalAlignment="Center"
                                        FontWeight="Bold"
                                        HorizontalTextAlignment="Center"
                                        Text="IdType" />
                                    <TextBlock
                                        Width="100"
                                        Margin="5,0,0,0"
                                        VerticalAlignment="Center"
                                        FontWeight="Bold"
                                        HorizontalTextAlignment="Center"
                                        Text="Type" />
                                    <TextBlock
                                        Width="150"
                                        Margin="5,0,0,0"
                                        VerticalAlignment="Center"
                                        FontWeight="Bold"
                                        HorizontalTextAlignment="Center"
                                        Text="Date" />
                                </StackPanel>
                            </ListView.Header>
                        </ListView>
                    </ScrollViewer>
                    <ListView
                        x:Name="ListViewLoginImage"
                        Height="400"
                        MinWidth="550"
                        AllowDrop="True"
                        BorderBrush="{ThemeResource ControlStrongStrokeColorDefaultBrush}"
                        BorderThickness="1"
                        CanDrag="True"
                        CanDragItems="True"
                        CanReorderItems="True"
                        DoubleTapped="ListViewLoginImage_DoubleTapped"
                        ItemTemplate="{StaticResource LoginImagesListViewTemplate}"
                        ItemsSource="{x:Bind ViewModel.TableLoginImages, Mode=TwoWay}"
                        RightTapped="ListViewLoginImage_RightTapped"
                        SelectedItem="{x:Bind ViewModel.SelectedLoginImage, Mode=TwoWay}"
                        SelectionMode="Single">
                        <ListView.ContextFlyout>
                            <MenuFlyout x:Name="ContextMenuLoginImage">
                                <MenuFlyoutItem
                                    x:Uid="AddLoginImage"
                                    Command="{x:Bind ViewModel.AddLoginImageCommand}"
                                    CommandParameter="{x:Bind}"
                                    Text="Add Image from file">
                                    <MenuFlyoutItem.Icon>
                                        <FontIcon Glyph="&#xe7aa;" />
                                    </MenuFlyoutItem.Icon>
                                </MenuFlyoutItem>
                                <MenuFlyoutItem
                                    x:Uid="DeleteLoginImage"
                                    Click="MenuFlyoutItemLoginImage_Clicked"
                                    DataContext="{Binding ElementName=ListViewLoginImage, Path=SelectedItem}"
                                    Tag="delete"
                                    Text="Delete Image">
                                    <MenuFlyoutItem.Icon>
                                        <FontIcon Glyph="&#xe711;" />
                                    </MenuFlyoutItem.Icon>
                                </MenuFlyoutItem>
                                <MenuFlyoutSeparator />
                                <MenuFlyoutItem
                                    x:Uid="ViewLoginImage"
                                    Click="MenuFlyoutItemLoginImage_Clicked"
                                    DataContext="{Binding ElementName=ListViewLoginImage, Path=SelectedItem}"
                                    Tag="view"
                                    Text="View Image">
                                    <MenuFlyoutItem.Icon>
                                        <FontIcon Glyph="&#xE8BA;" />
                                    </MenuFlyoutItem.Icon>
                                </MenuFlyoutItem>
                                <MenuFlyoutItem
                                    Click="MenuFlyoutItemLoginImage_Clicked"
                                    DataContext="{Binding ElementName=ListViewLoginImage, Path=SelectedItem}"
                                    Tag="view2"
                                    Text="View Image2">
                                    <MenuFlyoutItem.Icon>
                                        <FontIcon Glyph="&#xE8BA;" />
                                    </MenuFlyoutItem.Icon>
                                </MenuFlyoutItem>
                            </MenuFlyout>
                        </ListView.ContextFlyout>
                    </ListView>
                </StackPanel>
            </StackPanel>

            <StackPanel
                Grid.Row="1"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Visibility="{x:Bind ViewModel.IsLoading, Mode=OneWay}">
                <ProgressRing IsActive="{x:Bind ViewModel.IsLoading, Mode=OneWay}" />
                <TextBlock x:Uid="Sql_Loading" />
            </StackPanel>

            <StackPanel
                Grid.Row="1"
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Visibility="{x:Bind ViewModel.HasFailures, Mode=OneWay}">
                <TextBlock
                    x:Uid="Sql_FailedMessage"
                    HorizontalAlignment="Center"
                    TextWrapping="WrapWholeWords" />
                <HyperlinkButton
                    x:Uid="Sql_Reload"
                    HorizontalAlignment="Center"
                    Command="{x:Bind ViewModel.ReloadCommand}" />
            </StackPanel>

        </Grid>

    </Grid>
</Page>

